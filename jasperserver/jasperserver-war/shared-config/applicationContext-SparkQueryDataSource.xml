<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">
       
	<bean id="sparkQueryDataSource" class="example.cdspro.SparkQueryDataSourceDefinition">
		<property name="factory" ref="customDataSourceServiceFactory"/>
		<property name="name" value="sparkQueryDataSource"/>
		<property name="dataAdapterClassName" value="net.sf.jasperreports.data.jdbc.JdbcDataAdapterImpl"/>
	</bean>
	
    <bean class="com.jaspersoft.jasperserver.api.common.util.spring.GenericBeanUpdater">
        <property name="definition" ref="addMessageCatalog"/>
        <property name="value">
            <list>
                <value>WEB-INF/bundles/sparkquery</value>
            </list>
        </property>
        <property name="valueType" value="stringList"/>
    </bean>
</beans>
